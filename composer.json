{
  "name": "maxo1991/php-api-client",
  "type": "wordpress-plugin",
  "description": "Reusable PHP API client for rate conversion.",
  "autoload": {
    "psr-4": {
      "PaymentAPI\\": "src/"
    }
  },
  "require": {
    "php": ">=8.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10"
  },
  "autoload-dev": {
    "psr-4": {
      "PaymentAPI\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit"
  }
}
